package com.maro.client.common.constant.enumconstant;

import com.maro.client.common.annotation.EnumSqlScript;

/**
 * 座位分组类型
 * @author 冯成果
 * @date 2018-3-26
 * @since 01.00.0001
 */
@EnumSqlScript(group = "seatGroupTypeEnum",groupName = "座位分组类型")
public enum SeatGroupTypeEnum {

    LOBBY(0, "大堂"),
    OUTSIDE(1, "场外"),
    ROOM(2, "包间");




    private String name;
    private Integer code;

    public String getName() {
        return name;
    }

    public Integer getCode() {
        return code;
    }
    SeatGroupTypeEnum(Integer code, String name){
        this.code = code;
        this.name = name;
    }

}
